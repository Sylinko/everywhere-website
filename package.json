    "fumadocs-core": "16.4.3",
    "fumadocs-mdx": "14.2.4",
    "fumadocs-ui": "16.4.3",
    "lucide-react": "^0.562.0",
    "next": "16.0.10",
    "next-themes": "^0.4.6",
    "octokit": "^5.0.5",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react": "^19.2.3",
